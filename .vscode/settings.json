{
    "python.interpreter.path": "./my_app_back/venv/bin/python",
    "python.defaultInterpreterPath": "./my_app_back/venv/bin/python",
    "python.terminal.activateEnvironment": true,
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.envFile": "${workspaceFolder}/my_app_back/.env",
    "python.testing.pytestEnabled": true,
    "python.testing.pytestPath": "./my_app_back/venv/bin/pytest",
    "python.testing.pytestArgs": [
        "./my_app_back/tests"
    ],
    "python.testing.cwd": "./my_app_back",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "./my_app_back/venv/bin/flake8",
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "./my_app_back/venv/bin/black",
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/venv": false
    },
    "terminal.integrated.cwd": "./my_app_back"
}